{
  "name": "vite-plugin-pinia-auto-store",
  "version": "1.0.0",
  "description": "A Vite plugin that generates Pinia store refs and keeps them in sync from a store directory.",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "pnpm -C playground dev",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "build": "tsdown",
    "prepare": "simple-git-hooks install",
    "example": "npx tsx examples/01-basic-example.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "deps:update": "taze -i -s",
    "release": "bumpp --tag --push --publish",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "changelog:full": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build",
    "publish:dry": "npm publish --dry-run"
  },
  "keywords": [
    "tsdown",
    "oxlint",
    "vitest",
    "tsx",
    "typescript",
    "pinia",
    "pinia-plugin",
    "vite-plugin-pinia-auto-store"
  ],
  "author": "skelanimals(https://github.com/webfanzc)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^25.0.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "^10.4.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "lint-staged": "^16.1.2",
    "oxlint": "^1.7.0",
    "pinia": "^3.0.3",
    "pnpm": "^10.13.1",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.9.2",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.17"
  },
  "peerDependencies": {
    "vite": "^7.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint:fix",
    "commit-msg": "npx --no commitlint --edit \"$1\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webfanzc/vite-plugin-pinia-auto-store.git"
  },
  "packageManager": "pnpm@10.11.0"
}
